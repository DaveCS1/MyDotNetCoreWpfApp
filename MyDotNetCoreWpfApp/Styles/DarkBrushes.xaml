<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush po:Freeze="True" x:Key="PageBackgroundBrush" Color="#FF000000" />
    <SolidColorBrush po:Freeze="True" x:Key="TextForegroundBrush" Color="#FFFFFFFF" />
    <SolidColorBrush po:Freeze="True" x:Key="FocusStyleBorderBrush" Color="#FFFFFFFF" />

    <!--Button-->
    <SolidColorBrush po:Freeze="True" x:Key="ButtonBackgroundBrush" Color="#FF333333" />
    <SolidColorBrush po:Freeze="True" x:Key="ButtonForegroundBrush" Color="#FFFFFFFF" />
    <SolidColorBrush po:Freeze="True" x:Key="ButtonBorderBrush" Color="#FFFFFFFF" />
</ResourceDictionary>